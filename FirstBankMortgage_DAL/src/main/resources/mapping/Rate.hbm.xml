<?xml version="1.0" encoding="UTF-8"?>
	<hibernate-mapping>
	    <class name = "domain.RateDomainModel" table = "tblRate">
	        
	    <id name="RateID" type="int">
			<column name="RATEID" length="9" not-null="true"/>
		</id>
		
	    <property name="MinCreditScore" type="int">
			<column name="MINCREDITSCORE" length="3" not-null="true" />
		</property>
		<property name="InterestRate" type="double">
			<column name="INTERESTRATE" length="5" not-null="true" />
		</property>
	        
	    </class>
	</hibernate-mapping>